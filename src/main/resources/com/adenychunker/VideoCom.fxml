<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.adenychunker.VideoComController">
   <children>
      <Button id="uploadBtn" fx:id="uploadBtn" layoutX="24.0" layoutY="66.0" mnemonicParsing="false" onAction="#fileChooserHandler" text="Upload" />
      <Label layoutX="24.0" layoutY="39.0" prefHeight="18.0" prefWidth="371.0" text="Upload any video to compress" />
      <ChoiceBox id="resChoice" fx:id="resChoiceBox" layoutX="24.0" layoutY="156.0" prefWidth="150.0" />
      <ChoiceBox id="bitrateChoice" fx:id="bitrateChoiceBox" layoutX="24.0" layoutY="212.0" prefWidth="150.0" />
      <Label layoutX="24.0" layoutY="132.0" prefHeight="18.0" prefWidth="186.0" text="Choose desired resolution" />
      <Label layoutX="24.0" layoutY="188.0" prefHeight="18.0" prefWidth="186.0" text="Choose Bitrate" />
      <Button id="compressBtn" fx:id="compressBtn" layoutX="24.0" layoutY="264.0" mnemonicParsing="false" onAction="#compressFinalVideoOutput" prefHeight="26.0" prefWidth="150.0" text="Compress" />
      <Button fx:id="backBtn" layoutX="26.0" layoutY="338.0" mnemonicParsing="false" onAction="#backToHomePage" text="Back" />
      
   </children>
</AnchorPane>
